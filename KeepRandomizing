using System;

namespace keeprandomizing
{
    class Program
    {
        static void Main(string[] args)
        {
            Random r = new Random();
            int n = r.Next(20);
            int posX, posY;
            for (int i = 0; i <= n; i++)
            {
                posX = r.Next(500);
                posY = r.Next(500);
                Console.WriteLine("X :" + posX + " Y :" + posY);
            }
        }
    }
}
