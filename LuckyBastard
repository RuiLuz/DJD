using System;

namespace LuckyBastard
{
    class Program
    {
        static void Main(string[] args)
        {
            Random r = new Random();
            int health = 100;
            health -= r.Next(0, 50);
            Console.WriteLine("O jogador tem " + health + " pontos de vida !");
            health -= r.Next(0, 49);
            Console.WriteLine("O jogador tem " + health + " pontos de vida !");
            health -= health - 1;
            Console.WriteLine("O jogador tem " + health + " ponto de vida !");
            Console.WriteLine("Lucky Bastard!");
        }
    }
}
